server {
	listen   80;
	server_name  daily.newdjangosite.com;

	access_log  /var/log/nginx/localhost.access.log;

	location / {
		root /var/www/python/newdjangosite-daily/config/shutdown;
	}

	error_page 404 /index.html;
}

